class Solution:
    def reverse(self, arr: list, n: int) -> None:


        #twopointer
        l=0
        r=n-1
        while l<=r:
            arr[l],arr[r]=arr[r],arr[l]
            l=l+1
            r=r-1
        return arr






        # t=n-1
        # for i in range(n//2):
        #     arr[i],arr[t]=arr[t],arr[i]
        #     t=t-1
        # return arr
