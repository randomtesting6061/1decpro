class Solution:
    def sumHighestAndLowestFrequency(self, nums):
        dict_count={}
        for i in nums:
            if i in dict_count:
                dict_count[i]=dict_count[i]+1
            else:
                dict_count[i]=1
        max_freq=0
        min_freq=len(nums) if nums else 0
      
        for j in dict_count:
            if dict_count[j]>max_freq:
               
                
                max_freq=dict_count[j]
                
            if dict_count[j]<min_freq:
                min_freq=dict_count[j]
        return min_freq+max_freq

            
